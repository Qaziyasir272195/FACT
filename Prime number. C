#include <stdio.h>
int main() {
    int i=1,n,sum=0;
    printf("enter a number");
    scanf("%d",&n);
    for(i=1;i<=n;i++){
    if(n%i==0){
    sum++;
    }
    }
    if(sum==2)
    {
    printf("%d is a prime no",n);
    }
    else
    {
    printf("%d is a not prime no ",n);
    }
    return 0;
    }
